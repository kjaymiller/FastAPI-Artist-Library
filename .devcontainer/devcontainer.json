{
"name": "FastAPI Demo Container",
"build": {
    "dockerfile": "../Dockerfile",
    "args": {
        "IMAGE": "python:3.11",
        "context": ".."
    }
},
"forwardPorts": [8000],
"customizations": {
    "vscode": {
        "extensions": [
            "ms-python.python",
            "bradlc.vscode-tailwindcss",
            "otovo-oss.htmx-tags",
            "ms-vscode.vscode-node-azure-pack",
            "wholroyd.jinja",
            "ms-azuretools.vscode-bicep"
        ]
    }
},
"postCreateCommand": "pip install -r requirements.txt"
}